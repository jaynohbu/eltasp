	function loadCheckValues(){
	
		if ("<%=SearchType%>"=="HBOL" || "<%=SearchType%>"=="" )
		{
			if("<%=vON_MAS%>" == "Y"){
				document.getElementById("check_Mas").checked=true;
				}
			if("<%=vON_SUB%>" == "Y"){
				document.getElementById("check_Sub").checked=true;
				}
			if ("<%=vON_MAS%>"!= "Y" && "<%=vON_SUB%>"!= "Y")
			{
			document.getElementById("Check_AL1").checked=true;
			}	
			
		}
		else
		{
			if ("<%=IsCont%>"== "yes")
			{
			document.getElementById("Check_Co").checked=true;
			}
			if ("<%=NotCont%>"== "yes")
			{
			document.getElementById("Check_Yes").checked=true;
			}
			if ("<%=NotCont%>"!= "yes" && "<%=IsCont%>"!= "yes")
			{
			document.getElementById("Check_Bo").checked=true;
			}
		}
		
			if("<%=MMS%>" == "Y"){
