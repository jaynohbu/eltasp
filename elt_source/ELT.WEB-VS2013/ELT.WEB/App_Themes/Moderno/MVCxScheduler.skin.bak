<dxmvc:MVCxScheduler runat="server">
    <Styles SelectionColor="Gainsboro" CssPostfix="Moderno" CssFilePath="~/App_Themes/Moderno/{0}/styles.css">
        <ResourceNavigator ButtonSpacing="4px">
        </ResourceNavigator>

        <ViewSelector ButtonSpacing="2px"></ViewSelector>
        <ViewNavigator ButtonSpacing="2px"></ViewNavigator>

        <FormEditors ButtonEditCellSpacing="0">
            <CalendarHeader Spacing="1px"></CalendarHeader>
            <CalendarFooter Spacing="16px"></CalendarFooter>
        </FormEditors>
    </Styles>

    <Images SpriteCssFilePath="~/App_Themes/Moderno/{0}/sprite.css">
        <LoadingPanel Url="Scheduler/Loading.gif"></LoadingPanel>
    </Images>
