<?xml version="1.0" encoding="utf-8"?>
<!--<PresetInfo PresetName="MSCRM" PresetDescription="WebCalendar MSCRM Look and Feel.  Sets both Look and Behavior properties." PresetType="All" />-->
<igsch:WebCalendar >
  <Layout DayNameFormat="FirstLetter" ShowYearDropDown="False" GridLineColor="#7288AC" ShowMonthDropDown="False" ShowGridLines="Both">
    <FooterStyle Cursor="Default" Font-Size="9pt" Font-Names="Verdana" BackColor="#D8D9DB" CustomRules="text-align:center">
      <BorderDetails WidthLeft="1px" StyleBottom="Solid" ColorBottom="Black" ColorRight="Black" WidthRight="1px" StyleRight="Solid" WidthBottom="1px" StyleLeft="Solid" ColorLeft="Black" />
    </FooterStyle>
    <SelectedDayStyle ForeColor="Black" />
    <OtherMonthDayStyle ForeColor="#CCCCCC" />
    <TodayDayStyle BorderWidth="2px" BorderColor="#E89F2E" BorderStyle="Solid" />
    <DayHeaderStyle Font-Size="9pt" Font-Names="Verdana" BackColor="#EEEEEE" />
    <TitleStyle Font-Size="9pt" Font-Names="Verdana" BackColor="#D8D9DB" />
  </Layout>
</igsch:WebCalendar>