<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<configuration>
    <system.web>
        <customErrors mode="On"/>
    </system.web>
    <system.webServer>
        <handlers>

            <add name="AsaxForbiddenHandler" verb="*" path="*.asax" type="System.Web.HttpForbiddenHandler, System.Web, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
            <add name="AscxForbiddenHandler" verb="*" path="*.ascx" type="System.Web.HttpForbiddenHandler, System.Web, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
            <add name="AshxForbiddenHandler" verb="*" path="*.ashx" type="System.Web.HttpForbiddenHandler, System.Web, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
            <add name="AsmxForbiddenHandler" verb="*" path="*.asmx" type="System.Web.HttpForbiddenHandler, System.Web, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
            <add name="AspxForbiddenHandler" verb="*" path="*.aspx" type="System.Web.HttpForbiddenHandler, System.Web, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />

        </handlers>
    </system.webServer>
</configuration>
