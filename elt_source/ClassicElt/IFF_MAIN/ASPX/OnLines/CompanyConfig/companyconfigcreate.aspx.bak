function TemplateUpdateControls(gn,ctrlId,cellId,value) {
    
	if(ctrlId==null) return true;
									  
	if(ctrlId=="txtTmpTitle" ||ctrlId=="txtTmpComment" )	{
			igtbl_getElementById(ctrlId).innerText=value;
			return true;
	}

	if(ctrlId=="txtTmpDate"){
		if(value=="")	{
			now = new Date();
			value= now.toLocaleDateString() + "(" + now.toLocaleTimeString() + ")";
		}
		igtbl_getElementById(ctrlId).innerText=value;
		return true;
	}
	
	if(ctrlId=="txtTmpEditedBy"){
		if(value=="")	{
			value= "<%=login_name%>";
		}
		igtbl_getElementById(ctrlId).innerText=value;
		return true;
	}


}

	
