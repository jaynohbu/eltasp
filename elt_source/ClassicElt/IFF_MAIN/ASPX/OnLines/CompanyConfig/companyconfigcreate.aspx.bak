function loadProfile(comboId) {

    var text  = document.getElementById( comboId + '_Text' );
    var combo = document.getElementById( comboId  );
    if( text ) {
        if(text.value=="") { 
//	        alert('Please input the search text.');
//	        return false; 
		__doPostBack("btnNew", "");   	
	        return false; 		

          }	    
	}
	else
	{
	        return false; 
	}
    
    
    var f = false;

    for(i = 0; i <= combo.options.length-1; i++) {
        if( text.value == combo.options[i].text) {
            f = true;
            break;
        }
	}
	
	if (f) {
	    document.getElementById( 'txtAccountNumber'  ).value = combo.value;
	    document.getElementById( 'txtDBA'  ).value = text.value;	
	    text.value = '';    
	    return true;
	}
	else
	{
        goDupChk(text.value)
  	    return false;
	}
	
}

