function chk_phone_no() {
    if (document.CompanyCreate.dlCountry.value != "US") return true;
    var obj = document.getElementById("txtBPhone");
    if(obj) {
	        if (obj.value != ""){
	            ok = confirm('Do you want to convert the Phone # with US format?');
	            if(!ok) return false;  
	        }
        }    
    obj = document.getElementById("txtCPhone");
    if(obj) {
	        if (obj.value != ""){
	            ok = confirm('Do you want to convert the Cell Phone # with US format?');
	            if(!ok) return false;  
	        }
        }    
    obj = document.getElementById("txtBFax");
    if(obj) {
	        if (obj.value != ""){
	            ok = confirm('Do you want to convert the Fax. # with US format?');
	            if(!ok) return false;  
	        }
        }    
        
return true;
}


