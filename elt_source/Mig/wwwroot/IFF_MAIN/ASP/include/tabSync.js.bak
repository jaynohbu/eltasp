function PerformGetWorkName(Url)
{
if ( Url == null ) return Url;
if ( Url.indexOf("?") >=0 ) {
	Url = Url.substring(0,Url.indexOf("?"));
}
var lastSlashIndex = Url.lastIndexOf("/");
if ( lastSlashIndex <= 0) return Url;
var parentVirPath = Url.substring(0, lastSlashIndex);
var strUrl1 = PerformValidName(Url);
var strUrl2 = PerformValidName(parentVirPath);
var tmpStr = strUrl2 + "/" + strUrl1;
return tmpStr.toLowerCase();
}

function PerformValidName(Url)
{
var lastSlashIndex = Url.lastIndexOf("/");
if(lastSlashIndex < 0) return Url;
var parentVirPath = Url.substring(0, lastSlashIndex);
var strUrl = Url.substring(lastSlashIndex+1);
return strUrl;
}

