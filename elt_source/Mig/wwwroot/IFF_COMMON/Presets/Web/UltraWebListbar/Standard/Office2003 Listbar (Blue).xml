<?xml version="1.0" encoding="utf-8"?>
<!--<PresetInfo PresetName="Office 2003 Listbar (Blue)" PresetDescription="Infragistics Office 2003 Listbar (Blue) Preset.  Sets both Look and Behavior properties." PresetType="All" />-->
<iglbar:UltraWebListbar BarWidth="100%" BorderStyle="None" MergeStyles="True" ItemSelectionStyle="IconOnlySelection" BorderWidth="4px">
  <DefaultGroupStyle Cursor="Default" Height="100%" BackColor="WhiteSmoke" />
  <DefaultItemStyle Cursor="Default" Font-Size="8pt" Font-Names="Verdana">
    <Padding Left="5px" />
    <Margin Left="1px" />
  </DefaultItemStyle>
  <DefaultItemHoverStyle BorderWidth="1px" BorderStyle="Outset">
    <Margin Left="0px" />
  </DefaultItemHoverStyle>
  <DefaultGroupButtonSelectedStyle Cursor="Default" ForeColor="Black" BackColor="Silver" />
  <DefaultItemSelectedStyle Cursor="Default" BorderWidth="1px" BorderStyle="Inset">
    <Margin Left="0px" />
  </DefaultItemSelectedStyle>
  <DefaultGroupButtonStyle Cursor="Default" Height="24px" Font-Size="10pt" Font-Names="Verdana" BackgroundImage="blueExplorer.gif">
    <Padding Left="3px" />
  </DefaultGroupButtonStyle>
  <DefaultGroupButtonHoverStyle BackgroundImage="orangeexplorer.gif" />
</iglbar:UltraWebListbar>